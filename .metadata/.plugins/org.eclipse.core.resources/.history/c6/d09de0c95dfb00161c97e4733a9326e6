
public class Ingredients {
	private String name;
	private int volume;
	
	public Ingredients(){
		
	}
	
	public void add(){
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getVolume() {
		return volume;
	}

	public void setVolume(int volume) {
		this.volume = volume;
	}
}

